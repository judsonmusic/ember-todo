{{outlet}}
<h1>Users</h1>
<hr>
<p>Below are the users from users.</p>
<div>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Address</th>
                <th>Phone</th>
            </tr>
        </thead>
        <tbody>
            {{#each model as |user|}}
            <tr>
                <td>{{user.name}}</td>
                <td>{{user.email}}</td>
                <td>{{user.address.street}} {{user.address.city}}, {{user.address.zipcode}}</td>
                <td>{{user.phone}}</td>
                <td>
                    {{!-- {{user.posts}} --}}
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>